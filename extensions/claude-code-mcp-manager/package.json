{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "claude-code-mcp-manager",
  "title": "Claude Code MCP Manager",
  "description": "Toggle Claude Code MCP plugins on/off",
  "icon": "extension-icon.png",
  "author": "doxxx93",
  "authorUrl": "https://github.com/doxxx93",
  "repository": {
    "type": "git",
    "url": "https://github.com/doxxx93/claude-code-mcp-manager"
  },
  "bugs": {
    "url": "https://github.com/doxxx93/claude-code-mcp-manager/issues"
  },
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "toggle-mcp",
      "title": "Toggle MCP",
      "subtitle": "Claude Code",
      "description": "Toggle Claude Code MCP plugins",
      "mode": "view"
    },
    {
      "name": "mcp-presets",
      "title": "MCP Presets",
      "subtitle": "Claude Code",
      "description": "Apply MCP presets (minimal, coding, all)",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.0",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.7",
    "eslint": "^8.57.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
